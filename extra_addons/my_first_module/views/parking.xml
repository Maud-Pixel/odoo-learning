<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <data>
        <record id="parking_form" model="ir.ui.view">
            <field name="name">parking.forms</field>
            <field  model="parking.parking">parking.parking</field>
            <field name="arch" type="xml">
                <form>
                    <field name="name"/>
                    <field name="cars_id"/>
                </form>
            </field>
        </record>

        <record id = "parking_tree" model="ir.ui.view">
            <field name="name">parking.forms</field>
            <field  model="parking.parking">parking.parking</field>
            <field name="arch" type="xml">
                <form>
                    <field name="name"/>
                    <field name="car_ids"/>
                </form>
            </field>
        </record>

        <record id ="parking_action" model="ir.action.act_window">
            <field name="name">parking.forms</field>
            <field name="type">on.act_window</field>
            <field name="res_model">parking.parking</field>
            <field name="view_mode">tree, form</field>
        </record>

        <menuitem id="parking_menu" name="Parking" parent ="car_menu"  sequence = "1" action="parking_action "/>

    </data>
</odoo>